Proceso sin_titulo
	Definir i, tabla, resultado como entero;
	
	Escribir "Escriba el valor de la tabla que desea conocer debe ser numero entero";
	Leer tabla;
	

	para i<-1 hasta 10 con paso 1 Hacer
		resultado <- tabla * i;
		
		Escribir " ", tabla, " * " ,i, " ="," ", resultado;
	
	FinPara
	
		
	
FinProceso
