Proceso Arreglos1
	
	Definir valores, indice, indice2 Como entero;
	
	Dimension valores[5];
	

	
	para indice <- 0 hasta 4 con paso 1 Hacer
		Escribir "Digite un valor (", indice, ")";
		leer valores[indice];
	FinPara
		escribir "[0]=",valores[0];
		escribir "[1]=",valores[1];
		escribir "[2]=",valores[2];
		escribir "[3]=",valores[3];
		escribir "[4]=",valores[4];
	
	
	
FinProceso
